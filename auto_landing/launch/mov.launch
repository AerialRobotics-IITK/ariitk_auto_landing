<?xml version="1.0"?>
<launch>
    <include file="$(find rotors_gazebo)/launch/mav.launch" >
        <arg name="mav_name" value="firefly" />
    </include> 

    <include file="$(find husky_gazebo)/launch/spawn_husky.launch">
        <arg name="x" value="2" />
        <arg name="y" value="2" />
    </include>

    <node pkg="auto_landing" type="auto_landing_node" name="auto_landing_node" output="screen">
        <rosparam command="load" file="$(find auto_landing)/cfg/husky_trajectory_params.yaml" />
    </node>

</launch>